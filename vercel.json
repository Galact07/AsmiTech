{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/hf/v1/chat/completions.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/save-translation.js": {
      "maxDuration": 10
    }
  }
}
